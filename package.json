{
  "name": "frame-account-panel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "dev": "vite --port 5174",
    "devs": "node scripts/devhttps.mjs",
    "build": "vite build",
    "lint": "eslint .",
    "ok": "npm run format:check && npm run type-check && npm run lint && npm run build",
    "preview": "vite preview",
    "up": "npx -y npm-check-updates@latest -u && npm i",
    "knip": "knip --production --fix --allow-remove-files --format --exports"
  },
  "dependencies": {
    "@artifact/client": "npm:@jsr/artifact__client@^0.0.140",
    "fast-deep-equal": "^3.1.3",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "knip": "^5.61.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-mkcert": "^1.17.8"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "trailingComma": "none"
  }
}
